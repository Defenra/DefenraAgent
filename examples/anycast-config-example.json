            "region": "eu-west",
            "priority": 0
          },
          {
            "id": "agent-us-east",
            "endpoint": "https://agent-us.example.com",
            "region": "us-east",
            "priority": 0
          },
          {
            "id": "agent-asia",
            "endpoint": "https://agent-asia.example.com",
            "region": "asia",
            "priority": 1
          }
        ],
        "maxHops": 3,
        "antiDDoS": {
          "enabled": true,
          "rateLimit": {
            "windowSeconds": 60,
            "maxRequests": 100
          },
          "blockDurationSeconds": 300
        }
      },
